{
    "projectName":"Duly-Noted",
    "files": [
        "./ts/typings/index.d.ts",
        "./ts/index.ts",
        "./ts/generators/htmlGenerator.ts",
        "./ts/generators/markdownGenerator.ts",
        "./ts/modules/referenceParser.ts",
        "./ts/classes/IConfig.ts",
        "./ts/classes/referenceCollection.ts",
        "./ts/helpers/helpers.ts",
        "./ts/helpers/fileType.ts",
        "./authors.md",
        "./license.md"
    ],
    "indexFile":"README.md",
    "outputDir": "./docs",
    "anchorRegExp": "!([a-zA-Z0-9\\-\\./]+)",
    "linkRegExp": "(?:^|\\W)@([a-zA-Z0-9\\-\\./]+)",
    "readme": "./README.md",
    "externalReferences": [
        {
            "anchor": "wiki",
            "path": "https://en.wikipedia.org/wiki/::"
        },
        {
            "anchor":"so",
            "path": "http://stackoverflow.com/questions/::"
        },
        {
            "anchor":"issue",
            "path": "https://github.com/ShieldMyFiles/duly-noted/issues/::"
        }
    ],
    "generators": [
        "markdown"
    ],
    "markdownGeneratorOptions": {
        "gitHubMarkdownAnchors": true,
        "htmlAnchors": true
    },
    "leaveJSONFiles":false
}