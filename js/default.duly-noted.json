{
    "projectName":"My Project's Name", // STEP 1. Set a project name
    "files": [
        // STEP 2. set some file globs for the files you want to generate documents for.
    ], 
    "outputDir": "./docs", // STEP 3. Set an output directory for the docs
    "commentRegExp": "\\/\\/(.*)", // Leave this if you want '//' being comments.
    "longCommentOpenRegExp": "\\/\\*\\*(.*)", // Leave this if you want '/**' starting long comments
    "longCommentLineRegExp": "\\*(.*)", // Leave this if you want '*' continuing long comments
    "longCommentCloseRegExp": "\\*\/", // Leave this if you want '*/' continuing long comments
    "anchorRegExp": "!([a-zA-Z0-9\\-\\./]+)", // Leave this if you want to use '!' to define anchors
    "linkRegExp": "(?:^|\\W)@([a-zA-Z0-9\\-\\./]+)", // Leave this if you want to use '@' to define links
    "readme": "./README.md", // STEP 4. Set this to your readme file
    "externalReferences": [
        {
            "anchor": "wiki",
            "path": "https://en.wikipedia.org/wiki/::"
        },
        {
            "anchor":"so",
            "path": "http://stackoverflow.com/questions/::"
        }

        // STEP 5 (optional) Add you own external links
    ],
    "generators": [ 
        // STEP 6 Pick a generator, or generators to use.
        "html"
    // ,"markdown"
    ]
}